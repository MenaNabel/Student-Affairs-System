@{
    ViewBag.Title = "toEditInformationStudent";
    Layout = "~/Views/Shared/_LayoutUniversity.cshtml";
}

    <section class="site-hero site-sm-hero overlay" data-stellar-background-ratio="0.5" style="background: url(/images/BG.jpg) no-repeat  fixed; background-size: cover;">
        <div class="container">
            <div class="row align-items-center justify-content-center site-hero-sm-inner">
                <div class="col-md-7 text-center">

                    <div class="mb-5 element-animate">
                        <div class="col-md-12">
                            <form method="get" action="~/studentInformation/editInformationStudent" class="subscribe">
                                <div class="form-group">
                                    <input type="text" name="student_ID" class="form-control py-2" required pattern="[0-9]{9}" size="9" placeholder="Student ID" title="Enter 9-digit only">


                                    <input type="submit" class="btn btn-primary submit" value="GO">
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
<!--End section-->


